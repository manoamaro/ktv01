<h1>Listing emissoras</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Canal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @emissoras.each do |emissora| %>
  <tr>
    <td><%= emissora.nome %></td>
    <td><%= emissora.canal %></td>
    <td><%= link_to 'Show', emissora %></td>
    <td><%= link_to 'Edit', edit_emissora_path(emissora) %></td>
    <td><%= link_to 'Destroy', emissora, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

