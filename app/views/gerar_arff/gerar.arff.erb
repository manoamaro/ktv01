@RELATION interacoes

<% @campos.each do |c,t| %>
@ATTRIBUTE <%= c %> <%= normalize_arff_type t %>
<% end %>

@DATA

<% @result.each do |r| %>
<%= r.collect do |i| 
      i
      if i.is_a? String 
        "'#{i}'"
      else
        if i.is_a? Time 
          "'#{to_date_time(i)}'"
        else
          i
        end
      end
    end.join(',') %>
<% end %>
