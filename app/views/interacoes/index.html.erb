<h1>Listing interacoes</h1>

<table>
  <tr>
    <th>Tempo</th>
    <th>Data</th>
    <th>Emissora</th>
    <th>Programa</th>
    <th>Stb</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interacoes.each do |interacao| %>
  <tr>
    <td><%= interacao.tempo %></td>
    <td><%= interacao.data %></td>
    <td><%= link_to interacao.programacao.emissora.nome, emissora_path(interacao.programacao.emissora) %></td>
    <td><%= link_to interacao.programa.nome, programa_path(interacao.programa) %></td>
    <td><%= interacao.stb.identifier %></td>
    <td><%= link_to 'Show', interacao %></td>
    <td><%= link_to 'Edit', edit_interacao_path(interacao) %></td>
    <td><%= link_to 'Destroy', interacao, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Interacao', new_interacao_path %>
